{
  "name": "words-in-word",
  "version": "0.0.1",
  "scripts": {
    "build": "set NODE_ENV='production' & webpack -p",
    "devserver": "webpack-dev-server --debug --hot --inline --content-base --port 8090 --host 0.0.0.0",
    "lint": "eslint src",
    "build_for_ci": "set NODE_ENV='CI' & webpack -p",
    "test": "node ./node_modules/nightwatch/bin/nightwatch -c ./nightwatch.config.js -e local",
    "test_on_ci": "npm run lint && npm run build_for_ci && node ./node_modules/nightwatch/bin/nightwatch -c ./nightwatch.config.js"
  },
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.7.2",
    "bootstrap": "^3.3.2",
    "jquery": "^2.1.3",
    "less": "^2.7.1",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bluebird": "^3.0.5",
    "css-loader": "*",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.1.2",
    "less-loader": "^2.2.3",
    "nightwatch": "*",
    "node-static": "*",
    "react-hot-loader": "*",
    "selenium-standalone": "^5.8.0",
    "style-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  }
}
